modules = ["python-3.11", "python3", "nodejs-20", "postgresql-16"]

[nix]
channel = "stable-25_05"
packages = ["alsa-lib", "at-spi2-atk", "atk", "bc", "cairo", "cargo", "dbus", "ffmpeg", "ffmpeg-full", "freetype", "gitFull", "glib", "glibcLocales", "gnupg", "iana-etc", "jq", "k6", "libiconv", "libxcrypt", "mesa", "nspr", "nss", "openssl", "pango", "pkg-config", "playwright-driver", "postgresql", "rustc", "xorg.libX11", "xorg.libXcomposite", "xorg.libXdamage", "xorg.libXfixes", "xorg.libxcb", "xsimd", "libgbm", "libxkbcommon"]

[deployment]
deploymentTarget = "autoscale"
run = ["gunicorn", "-k", "gevent", "--timeout", "120", "--bind", "0.0.0.0:5000", "main:app"]


[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "gunicorn --bind 0.0.0.0:5000 --reuse-port --reload main:app"
waitForPort = 5000

[agent]
integrations = ["flask_stripe:1.0.0", "google-calendar:1.0.0"]

